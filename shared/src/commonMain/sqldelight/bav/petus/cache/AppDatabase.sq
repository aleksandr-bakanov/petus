import kotlin.Boolean;

CREATE TABLE Pets (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    isDead INTEGER AS Boolean DEFAULT 0 NOT NULL,
    petData TEXT NOT NULL
);

insertPet:
INSERT INTO Pets(id, isDead, petData)
VALUES(?, ?, ?);

removeAllPets:
DELETE FROM Pets;

selectAllPets:
SELECT Pets.*
FROM Pets;
